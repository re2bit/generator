{
  "name": "re2bit/generator",
  "description": "Code Gen",
  "require": {
    "php": "^7.4",
    "doctrine/annotations": "^1.6",
    "doctrine/collections": "^1.6",
    "doctrine/inflector": "^1.3",
    "jms/serializer": "^2.2",
    "symfony/console": "^3.3",
    "symfony/process": "^3.1.4",
    "symfony/validator": "^4.3",
    "twig/twig": "^2.11",
    "fzaninotto/faker": "^1.8"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.9",
    "friendsofphp/php-cs-fixer": "^2.16",
    "captainhook/captainhook": "^5.3"
  },
  "bin": ["codegen"],
  "autoload": {
    "psr-4": {
      "Re2bit\\Generator\\": "./src"
    }
  },
  "scripts": {
    "test": [
      "vendor/bin/phpstan analyse --level 8 -c phpstan.neon",
      "vendor/bin/php-cs-fixer --config=.php_cs.dist fix --verbose --dry-run"
    ],
    "fixcs": [
      "vendor/bin/php-cs-fixer --config=.php_cs.dist fix --verbose"
    ]
  }
}
